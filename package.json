{
  "name": "michael-pumo",
  "type": "module",
  "private": true,
  "scripts": {
    "sb:components": "storyblok pull-components --space 338175",
    "sb:types": "npm run sb:components && storyblok generate-typescript-typedefs --sourceFilePaths ./components.338175.json --destinationFilePath ./types/storyblok.d.ts",
    "dev": "npm run sb:types && nuxt dev --host",
    "dev:ssl": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "nuke": "del-cli '.output' 'dist' '.nuxt' 'node_modules' 'package-lock.json'"
  },
  "dependencies": {
    "@nuxt/scripts": "^0.11.8",
    "@pinia/nuxt": "^0.11.1",
    "@storyblok/nuxt": "^7.0.1",
    "@tailwindcss/vite": "^4.1.8",
    "@unhead/vue": "^2.0.10",
    "@vueuse/core": "^13.3.0",
    "gsap": "^3.13.0",
    "keen-slider": "^6.8.6",
    "lenis": "^1.3.4",
    "lucide-vue-next": "^0.513.0",
    "nuxt": "^3.17.5",
    "pinia-plugin-persistedstate": "^4.3.0",
    "postcss-nested": "^7.0.2",
    "tailwindcss": "^4.1.8",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.3",
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/image": "^1.10.0",
    "@nuxtjs/sitemap": "^7.3.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.28.0",
    "typescript": "^5.8.3",
    "vite-svg-loader": "^5.1.0"
  }
}
