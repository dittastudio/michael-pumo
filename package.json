{
  "name": "michael-pumo",
  "type": "module",
  "private": true,
  "scripts": {
    "sb:login": "storyblok login",
    "sb:logout": "storyblok logout",
    "sb:help": "storyblok --help",
    "sb:components": "storyblok components pull --space 338175 --verbose",
    "sb:types": "storyblok types generate --space 338175 --strict",
    "dev": "npm run sb:components && npm run sb:types && nuxt dev --host",
    "dev:ssl": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "nuke": "del-cli '.output' 'dist' '.nuxt' 'node_modules' 'package-lock.json'"
  },
  "dependencies": {
    "@nuxt/scripts": "^0.11.13",
    "@pinia/nuxt": "^0.11.2",
    "@storyblok/nuxt": "^8.0.4",
    "@tailwindcss/vite": "^4.1.12",
    "@unhead/vue": "^2.0.14",
    "@vueuse/core": "^13.8.0",
    "gsap": "^3.13.0",
    "keen-slider": "^6.8.6",
    "lenis": "^1.3.9",
    "lucide-vue-next": "^0.542.0",
    "nuxt": "^4.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "postcss-nested": "^7.0.2",
    "tailwindcss": "^4.1.12",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/image": "^1.11.0",
    "@nuxtjs/sitemap": "^7.4.3",
    "@total-typescript/ts-reset": "^0.6.1",
    "concurrently": "^9.2.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.34.0",
    "storyblok": "^4.4.0",
    "typescript": "^5.9.2",
    "vite-svg-loader": "^5.1.0"
  }
}
